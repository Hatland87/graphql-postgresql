generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Course {
  code    String
  id      Int       @default(autoincrement()) @id
  name    String
  lectures Lecture[]
}

model Lecture {
  courseId Int
  duration Int
  id       Int      @default(autoincrement()) @id
  teacher  String
  time     DateTime
  title    String
  course   Course   @relation(fields: [courseId], references: [id])
}